	<queryString language="SQL">
		<![CDATA[select ROW_NUMBER() OVER (ORDER BY t.sigla, p.nome) AS seq,
ROW_NUMBER () OVER ( PARTITION BY t.sigla ORDER BY t.sigla, p.nome) as nr,
t.sigla as turma, p.numero_registro, p.cpf, p.nome, ts.nome as situacao, p.email, cast(null as varchar(50)) as assinatura
from credito c
left join contrato ct on (c.id_contrato = ct.id)
left join matricula m on (ct.id_matricula = m.id)
left join pessoa p on (m.id_pessoa = p.id)
left join turma t on (c.id_turma = t.id)
left join tipo_situacao_contrato ts on (ct.id_tipo_situacao_contrato = ts.id)
where t.id_periodo_execucao = $P{ID_PERIODO_EXECUCAO} --idPeriodoExecucaoSelecionado RN003 (Obrigatorio todas consultas) 
and ct.id_tipo_situacao_contrato in  ($P!{IDS_TIPO_SITUACAO_CONTRATO}) --situacoesSelecionadas RN005 (Obrigatorio todas consultas)
group by t.sigla, p.numero_registro, p.cpf, p.nome, p.email, ts.nome order by t.sigla, p.nome]]>
	</queryString>
